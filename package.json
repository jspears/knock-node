{
  "name": "@speajus/knocklabs_node",
  "version": "0.4.26",
  "description": "Library for interacting with the Knock API",
  "homepage": "https://github.com/knocklabs/knock-node",
  "author": "@knocklabs",
  "license": "MIT",
  "keywords": [
    "knock",
    "knocklabs",
    "notify"
  ],
  "main": "./dist/src/index.js",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knocklabs/knock-node.git"
  },
  "bugs": {
    "url": "https://github.com/knocklabs/knock-node/issues"
  },
  "scripts": {
    "build": "$npm_execpath generate:version && tsc -p tsconfig.json && denoify",
    "postversion": "echo \"// autogenerated file do not modify\nexport const VERSION='$npm_package_version';\n\" > src/version.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:deno": "deno run --allow-net --allow-env --allow-read --allow-write --unstable  deno_dist/index.ts",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^15.0.1",
    "@types/pluralize": "0.0.29",
    "axios-mock-adapter": "1.19.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "denoify": "^1.5.3",
    "jsonwebtoken": "^9.0.0"
  }
}
